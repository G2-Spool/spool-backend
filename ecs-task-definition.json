{
    "family": "spool-interview-task",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::560281064968:role/ecsTaskExecutionRole",
    "taskRoleArn": "arn:aws:iam::560281064968:role/ecsTaskRole",
    "containerDefinitions": [
        {
            "name": "interview-service",
            "image": "560281064968.dkr.ecr.us-east-1.amazonaws.com/spool-interview:latest",
            "cpu": 256,
            "memory": 512,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:8080/health || exit 1"
                ],
                "interval": 30,
                "timeout": 5,
                "retries": 3,
                "startPeriod": 60
            },
            "environment": [
                {
                    "name": "PORT",
                    "value": "8080"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/spool-interview",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}